{
  "name": "python-visual-cli",
  "displayName": "python-visual-cli",
  "description": "",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "visual-solution.add",
        "title": "Add",
        "icon": {
          "light": "assets/light/add.svg",
          "dark": "assets/dark/add.svg"
        }
      },
      {
        "command": "visual-solution.delete",
        "title": "Delete"
      },
      {
        "command": "visual-solution.edit",
        "title": "Edit",
        "icon": {
          "light": "assets/light/edit.svg",
          "dark": "assets/dark/edit.svg"
        }
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "visual-solution-list",
          "name": "Solutions",
          "icon": "assets/logo.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "visual-solution.add",
          "when": "view == visual-solution-list",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "visual-solution.delete",
          "when": "view == visual-solution-list && viewItem == solution"
        },
        {
          "command": "visual-solution.edit",
          "when": "view == visual-solution-list && viewItem == solution"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package-win": "npm run package && vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.197",
    "@types/lokijs": "^1.5.8",
    "@types/uuid": "^9.0.2",
    "lodash": "^4.17.21",
    "lokijs": "^1.5.12",
    "python-shell": "^5.0.0",
    "uuid": "^9.0.0"
  }
}
